package practise;

import java.util.Scanner;

public class countevenodd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
System.out.println("Enetr the number");
int num=sc.nextInt();

int evencount=0;
int oddcount=0;

while(num>0) {
	int rem=num%10;{
		
		if(rem%2==0) {
			evencount++;
		}
		else {
			oddcount++;
		}
	}
num=num/10;
	
}

	}

}
